msgid "<link=\"FROZENOILFLOATER\">Frozen Slickster Egg</link>"
msgstr "<link=\"FROZENOILFLOATER\">Œuf de gluant givré</link>"

#. ILoveSlicksters.PHO_STRINGS.VARIANT_FROZEN.NAME
